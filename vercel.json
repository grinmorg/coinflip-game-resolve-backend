{
	"version": 2,
	"builds": [
		{
			"src": "src/main.ts",
			"use": "@vercel/node",
			"config": {
				"includeFiles": ["dist/**"]
			}
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "src/main.ts",
			"methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
		}
	],
	"rewrites": [
		{
			"source": "/docs",
			"destination": "/docs/index.html"
		},
		{
			"source": "/docs/(.*)",
			"destination": "/docs/$1"
		}
	]
}
