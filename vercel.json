{
	"version": 2,
	"builds": [
		{
			"src": "src/main.ts",
			"use": "@vercel/node",
			"config": {
				"includeFiles": ["dist/**"]
			}
		}
	],
	"rewrites": [
		{
			"source": "/docs",
			"destination": "/docs/index.html"
		},
		{
			"source": "/docs/(.*)",
			"destination": "/docs/$1"
		},
		{
			"source": "/api/(.*)",
			"destination": "src/main.ts"
		},
		{
			"source": "/(.*)",
			"destination": "src/main.ts"
		}
	]
}
